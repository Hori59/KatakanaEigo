<!-- <div class=""> -->
  <%= form_with url:'/search', method: :get, local: true, id: 'search_field' do |f| %>
    <%= f.text_field :search, placeholder: "キーワードを入力" %>
    <%= f.button :type => "submit", id: "search_submit" do %>
      <span class="glyphicon glyphicon-search" aria-hidden="true"></span>
    <% end %>
  <% end %>
